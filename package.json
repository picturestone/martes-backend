{
  "name": "martes-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "build:backup": "ts-node -T scripts/moveFiles.ts bin temp",
    "build:restore": "ts-node -T scripts/moveFiles.ts temp bin",
    "build:copy": "ts-node -T scripts/copyFiles.ts",
    "build:tsc": "tsc --build tsconfig.json",
    "build": "npm run build:backup && rimraf bin && npm run build:tsc && npm run build:restore && npm run build:copy && (cd bin && npm install)",
    "start": "(cd bin && node index.js)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/picturestone/martes-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/picturestone/martes-backend/issues"
  },
  "homepage": "https://github.com/picturestone/martes-backend#readme",
  "dependencies": {
    "bufferutil": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mqtt": "^4.2.6",
    "socket.io": "^4.1.2",
    "sqlite3": "^5.0.2",
    "utf-8-validate": "^5.0.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/node": "^15.0.1",
    "@types/sqlite3": "^3.1.7",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
